{
    "type": "library",
    "name": "nguyenanhung/my-crawler",
    "description": "Crawler helper, library - Basic, Simple and Lightweight",
    "keywords": [
        "crawler",
        "helper",
        "library",
        "php"
    ],
    "homepage": "https://github.com/nguyenanhung/my-crawler",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Nguyen An Hung",
            "email": "dev@nguyenanhung.com",
            "homepage": "https://nguyenanhung.com",
            "role": "Developer"
        }
    ],
    "require": {
